    <!--Openbox reconfigure-->
    <keybind key="A-C-o">
      <action name="Execute">
        <command>openbox --reconfigure</command>
      </action>
    </keybind>
    <!--Firefox-->
    <keybind key="W-w">
      <action name="Execute">
        <command>firefox</command>
      </action>
    </keybind>
    <!--Telegram-->
    <keybind key="W-t">
      <action name="Execute">
        <command>/opt/Telegram/Telegram -startintray</command>
      </action>
    </keybind>
    <!--Nemo-->
    <keybind key="W-e">
      <action name="Execute">
        <command>nemo</command>
      </action>
    </keybind>
    <!--GIMP-->
    <keybind key="W-g">
      <action name="Execute">
        <command>gimp</command>
      </action>
    </keybind>
    <!--SMPlayer-->
    <keybind key="A-m">
      <action name="Execute">
        <command>smplayer</command>
      </action>
    </keybind>
    <keybind key="A-C-w">
      <action name="Execute">
        <command>feh --randomize --bg-fill /opt/Wallpapers/</command>
      </action>
