    <!-- Resize -->
    <!--<keybind key="W-S-Right"><action name="ResizeRelative"><right>20</right></action></keybind>-->
    <!--<keybind key="W-S-Left"><action name="ResizeRelative"><right>-20</right></action></keybind>-->
