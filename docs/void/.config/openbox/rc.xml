        <y>0</y>
      </action>
    </keybind>
    <keybind key="W-Left">
      <action name="MoveRelative">
        <x>-20</x>
        <y>0</y>
      </action>
    </keybind>
    <keybind key="W-Down">
      <action name="MoveRelative">
        <x>0</x>
        <y>-20</y>
      </action>
    </keybind>
    <keybind key="W-Up">
      <action name="MoveRelative">
        <x>0</x>
        <y>20</y>
      </action>
