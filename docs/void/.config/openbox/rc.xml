    <keybind key="A-Escape">
      <action name="Lower"/>
      <action name="FocusToBottom"/>
      <action name="Unfocus"/>
    </keybind>
    <keybind key="XF86AudioRaiseVolume">
      <action name="Execute">
        <command>amixer set Master 5%+</command>
      </action>
    </keybind>
    <keybind key="XF86AudioLowerVolume">
      <action name="Execute">
        <command>amixer set Master 5%-</command>
      </action>
    </keybind>
    <keybind key="XF86AudioMute">
      <action name="Execute">
        <command>amixer set Master toggle</command>
      </action>
    </keybind>
    <keybind key="XF86AudioStop">
      <action name="Execute">
        <command>playerctl stop</command>
      </action>
    </keybind>
    <keybind key="XF86AudioPlay">
      <action name="Execute">
        <command>playerctl play-pause</command>
      </action>
    </keybind>
    <keybind key="XF86AudioPrev">
      <action name="Execute">
        <command>playerctl previous</command>
      </action>
    </keybind>
    <keybind key="XF86AudioNext">
      <action name="Execute">
        <command>playerctl next</command>
      </action>
    </keybind>
    <keybind key="XF86MonBrightnessUp">
      <action name="Execute">
        <command>xbacklight +10</command>
      </action>
    </keybind>
    <keybind key="XF86MonBrightnessDown">
      <action name="Execute">
        <command>xbacklight -10</command>
      </action>
    </keybind>
    <!-- ATALHOS DE TECLADO-->
    <!--Geany-->
    <keybind key="A-b">
      <action name="Execute">
        <command>geany</command>
      </action>
    </keybind>
    <!--jDownloader-->
    <keybind key="W-j">
      <action name="Execute">
        <command>/bin/sh "/opt/jDownloader/JDownloader2"</command>
      </action>
    </keybind>
    <!--Flameshot-->
    <keybind key="W-p">
      <action name="Execute">
        <command>flameshot gui</command>
      </action>
    </keybind>
    <!--captura tela delay 4s-->
    <keybind key="W-C-p">
      <action name="Execute">
        <command>flameshot full -d 4000 -c</command>
      </action>
    </keybind>
    <!--captura janela delay 4s-->
    <keybind key="A-W-p">
      <action name="Execute">
        <command>flameshot screen -d 4000 -c</command>
      </action>
    </keybind>
    <!--HandBrake-->
    <keybind key="A-h">
      <action name="Execute">
        <command>ghb</command>
      </action>
    </keybind>
    <!--Transmission-->
    <keybind key="A-t">
      <action name="Execute">
        <command>transmission-gtk</command>
      </action>
    </keybind>
    <!--Wallpapers-->
    <keybind key="A-w">
      <action name="Execute">
        <command>feh -. -x -g 800x500 /opt/Wallpapers/</command>
      </action>
    </keybind>
    <!--gmrun-->
    <keybind key="A-z">
      <action name="Execute">
        <command>gmrun</command>
      </action>
    </keybind>
    <!--Terminal-->
    <keybind key="A-C-t">
      <action name="Execute">
        <command>kitty</command>
      </action>
    </keybind>
    <!--Htop-->
    <keybind key="A-C-h">
      <action name="Execute">
        <command>kitty -e htop</command>
      </action>
